#include<stdio.h>
main()
{
	int nst=5,nsp=1;
	for(int i=1;i<=5;i++){
		for(int j=1;j<=nst;j++){
			printf("*");
		}
		for(int k=2;k<=nsp;k++){
			printf("  ");
		}
		for(int z=1;z<=nst;z++){
			printf("*");
		}
		nst--;
		nsp++;
		printf("\n");
	}
	int nt=1,np=5;
	for(int i=1;i<=5;i++){
		for(int j=1;j<=nt;j++){
			printf("*");
		}
		for(int k=2;k<=np;k++){
			printf("  ");
		}
		for(int z=1;z<=nt;z++){
			printf("*");
		}
		nt++;
		np--;
		printf("\n");
	}
}
